# Production Environment Variables for FinSay
# Copy this file to .env and fill in your actual production values

# Application
NODE_ENV=production
VITE_APP_NAME=FinSay
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_URL=https://api.finsay.app
VITE_APP_URL=https://finsay.app

# Groq API for Speech-to-Text (Production)
VITE_GROQ_API_KEY=your_production_groq_api_key_here

# Supabase Configuration (Production)
VITE_SUPABASE_URL=your_production_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_production_supabase_anon_key_here
VITE_SUPABASE_DB_PASSWORD=your_production_supabase_db_password_here

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token_here

# Error Tracking (Optional)
VITE_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
VITE_ENABLE_VOICE_FEATURES=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true